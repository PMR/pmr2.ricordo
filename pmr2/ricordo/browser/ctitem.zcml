<configure 
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="pmr2.ricordo">

  <browser:page
      for="pmr2.ricordo.interfaces.IQRItem"
      name="Default"
      class=".ctitem.DefaultView"
      permission="zope2.View"
      />

  <browser:page
      for="pmr2.ricordo.interfaces.IQRItem"
      name="Workspace"
      class=".ctitem.WorkspaceView"
      permission="zope2.View"
      />

  <browser:page
      for="pmr2.ricordo.interfaces.IQRItem"
      name="ExposureFile"
      class=".ctitem.ExposureFileView"
      permission="zope2.View"
      />

</configure>
